import Std.Nat
import Std.Eq

def add_mul (a b c : Nat) : a + b * c = a + (b * c) := Eq.refl Nat (a + b * c)
def mul_add (a b c : Nat) : a * b + c = (a * b) + c := Eq.refl Nat (a * b + c)
def mul_sub (a b c : Nat) : a * b - c = (a * b) - c := Eq.refl Nat (a * b - c)
def sub_mul (a b c : Nat) : a - b * c = a - (b * c) := Eq.refl Nat (a - b * c)
def add_sub (a b c : Nat) : a + b - c = (a + b) - c := Eq.refl Nat (a + b - c)
def sub_add (a b c : Nat) : a - b + c = (a - b) + c := Eq.refl Nat (a - b + c)
