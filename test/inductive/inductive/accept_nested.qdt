inductive List (A : Type) where
  | nil
  | cons (head : A) (tail : List A)

inductive Sexp (A : Type) where
  | atom : A → Sexp A
  | list : List (Sexp A) → Sexp A

inductive Rose (A : Type) where
  | rose : A → List (Rose A) → Rose A

inductive Foo where
  | mk : List Foo → Foo

