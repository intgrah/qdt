inductive List (A : Type) where
  | nil
  | cons (head : A) (tail : List A)

inductive Pair (A B : Type) where
  | mk (fst : A) (snd : B)

inductive Either (A B : Type) where
  | left (val : A)
  | right (val : B)

inductive Tree (A : Type) where
  | leaf
  | node (left : Tree A) (val : A) (right : Tree A)
