import Std.Eq

structure Sigma.{u, v} (A : Type u) (B : A → Type v) : Type (max u v) where
  (fst : A)
  (snd : B fst)

def Sigma.eta.{u, v} (A : Type u) (B : A → Type v) (p : Sigma.{u, v} A B) : Sigma.mk.{u, v} A B (Sigma.fst.{u, v} A B p) (Sigma.snd.{u, v} A B p) = p :=
  Eq.refl.{max u v} (Sigma.{u, v} A B) p
