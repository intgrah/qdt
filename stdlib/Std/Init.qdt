import Std.Eq

def id (A : Type) (x : A) : A := x
def const (A B : Type) (a : A) (b : B) := a
def comp (A B C : Type) (f : B → C) (g : A → B) (x : A) : C := f (g x)

def comp_assoc
  (A B C D : Type)
  (f : C → D) (g : B → C) (h : A → B) :
    comp A B D (comp B C D f g) h = comp A C D f (comp A B C g h) :=
  Eq.refl (A → D) (comp A B D (comp B C D f g) h)

-- Function extensionality axiom
def funext (A B : Type) (f g : A → B) (h : (x : A) → f x = g x) : f = g := sorry
